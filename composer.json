{
  "name": "stadline/linkdata-client",
  "description": "A Symfony4 bundle for Linkdata",
  "type": "symfony-bundle",
  "license": "proprietary",
  "authors": [
    {
      "name": "Raphael Bonini",
      "email": "raphael.bonini@stadline.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "guzzlehttp/guzzle": "^6.3",
    "pagerfanta/pagerfanta": "^1.0",
    "doctrine/inflector": "^1.3",
    "symfony/serializer": "^3.4",
    "doctrine/annotations": "^1.6"
  },
  "autoload": {
    "psr-4": { "Stadline\\LinkdataClient\\src\\": "src" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^0.9.2",
    "friendsofphp/php-cs-fixer": "^2.12"
  }
}
